{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2016",
    "jsx": "preserve",
    "checkJs": true,
    "baseUrl": "./src",
    "paths": {
      "i18n/*": ["i18n/*"],
      "view/*": ["view/*"],
      "config/*": ["config/*"],
      "security/*": ["security/*"],
      "modules/*": ["modules/*"]
    }
  },
  "exclude": ["node_modules", "**/node_modules/*"]
}
